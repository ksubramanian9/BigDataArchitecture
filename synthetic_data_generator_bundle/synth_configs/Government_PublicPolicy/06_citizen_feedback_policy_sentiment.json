        "mean": 3.0,
        "stddev": 0.6
      },
      {
        "name": "noise_db",
        "type": "float",
        "distribution": "normal",
        "mean": 65,
        "stddev": 10
      },
      {
        "name": "traffic_flow",
        "type": "int",
        "min": 0,
        "max": 5000,
        "skew": 1.1
      },
      {
        "name": "incident_flag",
        "type": "bool",
        "p_true": 0.02
      },
      {
        "name": "key",
        "type": "derived_concat",
