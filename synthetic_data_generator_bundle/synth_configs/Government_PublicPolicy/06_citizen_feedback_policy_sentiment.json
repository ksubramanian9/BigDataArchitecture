            "8": 1.5,
            "9": 1.5,
            "18": 1.5,
            "19": 1.5,
            "_default": 1.0
          },
          "day_of_week": {
            "5": 0.8,
            "6": 0.7,
            "_default": 1.0
          }
        }
      },
      {
        "name": "city",
        "type": "category",
        "choices": [
          {
            "value": "Bengaluru",
            "weight": 3
          },
          {
            "value": "Chennai",
            "weight": 2
          },
          {
            "value": "Mumbai",
            "weight": 2
          },
          {
            "value": "Delhi",
            "weight": 2
          },
          {
            "value": "Hyderabad",
            "weight": 1
          }
        ]
      },
      {
        "name": "sensor_type",
        "type": "category",
        "choices": [
          {
            "value": "air_quality",
            "weight": 4
          },
          {
            "value": "traffic",
            "weight": 3
          },
          {
            "value": "noise",
            "weight": 2
          },
          {
            "value": "water",
            "weight": 1
          }
        ]
      },
      {
        "name": "geo",
        "type": "geo",
        "bbox": [
          12.8,
          77.45,
          13.1,
          77.8
