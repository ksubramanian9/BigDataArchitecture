{
  "seed": 45,
  "entity_count": 200000,
  "time_window": {
    "start": "2025-01-01T06:00:00",
    "end": "2025-02-28T23:59:59"
  },
  "output": {
    "format": "csv"
  },
  "schema": {
    "fields": [
      {
        "name": "event_id",
        "type": "uuid"
      },
      {
        "name": "event_time",
        "type": "datetime",
        "format": "iso",
        "mode": "seasonality",
        "profile": {
          "hour_of_day": {
            "8": 1.5,
            "9": 1.5,
            "18": 1.5,
            "19": 1.5,
            "_default": 1.0
          },
          "day_of_week": {
            "5": 0.8,
            "6": 0.7,
            "_default": 1.0
          }
        }
      },
      {
        "name": "city",
        "type": "category",
        "choices": [
          {
            "value": "Bengaluru",
            "weight": 3
          },
          {
            "value": "Chennai",
            "weight": 2
          },
          {
            "value": "Mumbai",
            "weight": 2
          },
          {
            "value": "Delhi",
            "weight": 2
          },
          {
            "value": "Hyderabad",
            "weight": 1
          }
        ]
      },
      {
        "name": "crop",
        "type": "category",
        "choices": [
          {
            "value": "Rice",
            "weight": 3
          },
          {
            "value": "Wheat",
            "weight": 2
          },
          {
            "value": "Maize",
            "weight": 1
          },
          {
            "value": "Sugarcane",
            "weight": 1
          }
        ]
      },
      {
        "name": "market",
        "type": "category",
        "conditional_on": "city",
        "conditional_table": {
          "Bengaluru": [
            "Yeshwanthpur",
            "KR_Market",
            "APMC_Yard"
          ],
          "Chennai": [
            "Koyambedu",
            "Parry",
            "Tambaram_Market"
          ],
          "Mumbai": [
            "Vashi",
            "Dadar",
            "Byculla"
          ],
          "Delhi": [
            "Azadpur",
            "Okhla",
            "Ghanta_Ghar"
          ],
          "Hyderabad": [
            "Bowenpally",
            "Gudimalkapur",
            "Miryalaguda"
          ],
          "_default": [
            "Central_Market"
          ]
        }
      },
      {
        "name": "soil_moisture",
        "type": "float",
        "distribution": "uniform",
        "min": 5.0,
        "max": 45.0
      },
      {
        "name": "soil_ph",
        "type": "float",
        "distribution": "normal",
        "mean": 6.5,
        "stddev": 0.6
      },
      {
        "name": "rain_mm",
        "type": "float",
        "distribution": "exponential",
        "lambda": 0.05
      },
      {
        "name": "temp_c",
        "type": "int",
        "min": 12,
        "max": 42,
        "skew": 1.0
      },
      {
        "name": "yield_kg",
        "type": "int",
        "min": 200,
        "max": 8000,
        "skew": 1.1,
        "null_prob": 0.05
      },
      {
        "name": "pest_flag",
        "type": "bool",
        "p_true": 0.03
      },
      {
        "name": "key",
        "type": "derived_concat",
        "parts": [
          "city",
          "crop",
          "market"
        ],
        "sep": "|"
      }
    ]
  }
}