        "name": "aml_suspect",
        "type": "bool",
        "p_true": 0.02
      },
      {
        "name": "burst_count_24h",
        "type": "int",
        "min": 1,
        "max": 50,
        "skew": 0.8
      },
      {
        "name": "key",
        "type": "derived_concat",
        "parts": [
          "account_id",
          "counterparty_country",
          "channel"
