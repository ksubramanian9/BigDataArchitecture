{
  "seed": 45,
  "entity_count": 200000,
  "time_window": {
    "start": "2025-01-01T06:00:00",
    "end": "2025-02-28T23:59:59"
  },
  "output": {
    "format": "csv"
  },
  "schema": {
    "fields": [
      {
        "name": "event_id",
        "type": "uuid"
      },
      {
        "name": "event_time",
        "type": "datetime",
        "format": "iso",
        "mode": "seasonality",
        "profile": {
          "hour_of_day": {
            "8": 1.5,
            "9": 1.5,
            "18": 1.5,
            "19": 1.5,
            "_default": 1.0
          },
          "day_of_week": {
            "5": 0.8,
            "6": 0.7,
            "_default": 1.0
          }
        }
      },
      {
        "name": "city",
        "type": "category",
        "choices": [
          {
            "value": "Bengaluru",
            "weight": 3
          },
          {
            "value": "Chennai",
            "weight": 2
          },
          {
            "value": "Mumbai",
            "weight": 2
          },
          {
            "value": "Delhi",
            "weight": 2
          },
          {
            "value": "Hyderabad",
            "weight": 1
          }
        ]
      },
      {
        "name": "event_type",
        "type": "category",
        "choices": [
          {
            "value": "video_play",
            "weight": 5
          },
          {
            "value": "quiz_submit",
            "weight": 1
          },
          {
            "value": "forum_post",
            "weight": 1
          },
          {
            "value": "assignment_upload",
            "weight": 1
          }
        ]
      },
      {
        "name": "student_id",
        "type": "string",
        "pattern": "STU{000000-999999}"
      },
      {
        "name": "course_id",
        "type": "string",
        "pattern": "COURSE{000-999}"
      },
      {
        "name": "session_seconds",
        "type": "float",
        "distribution": "lognormal",
        "mean": 0.0,
        "stddev": 1.0
      },
      {
        "name": "late_submission",
        "type": "bool",
        "p_true": 0.08
      },
      {
        "name": "at_risk_label",
        "type": "bool",
        "p_true": 0.12
      },
      {
        "name": "key",
        "type": "derived_concat",
        "parts": [
          "student_id",
          "course_id"
        ],
        "sep": "|"
      }
    ]
  }
}